{
  "mappings": {
    "vehicle": {
      "properties": {
        "uuid": {
          "type": "string",
          "index": "not_analyzed"
        },
        "category": {
          "type": "string",
          "index": "not_analyzed"
        },
        "model": {
          "type": "string",
          "index": "not_analyzed"
        },
        "status": {
          "type": "string",
          "index": "not_analyzed"
        }
      },
      "dynamic_templates": [
        {
          "string_as_date": {
            "match_mapping_type": "string",
            "match":   "*_date",
            "mapping": {
              "type": "date"
            }
          }
        }
      ]
    }
  }
}